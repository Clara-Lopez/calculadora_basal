function cualEsMayor(num1, num2, num3) {
    let mayor;

    if (num1 >= num2 && num1 >= num3) {
        mayor = num1;
    } else if (num2 >= num1 && num2 >= num3) {
        mayor = num2;
    } else {
        mayor = num3;
    }

    return mayor;
}

// Ejemplo de uso
console.log(cualEsMayor(1, 2, 3));
